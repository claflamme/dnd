{
  "baseUrl": "/dnd/",
  "locals": {
    "url": "http://localhost:8080",
    "name": "D&D 5e Tools",
    "owner": "Someone",
    "description": "Keep rollin, rollin, rollin"
  },
  "output": "./docs",
  "plugins": [
    "wintersmith-coffee",
    "wintersmith-stylus"
  ],
  "require": {
    "moment": "moment",
    "_": "lodash",
    "typogr": "typogr"
  },
  "ignore": [
    "**/_*"
  ],
  "jade": {
    "pretty": true
  },
  "markdown": {
    "smartLists": true,
    "smartypants": true
  },
  "paginator": {
    "perPage": 3
  }
}
